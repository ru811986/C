#include <stdio.h>

int main() {
    float fahr, celsius;
    int lower, upper, step;
    
    lower = 0;
    upper = 100;
    step = 2;
    
    printf("Tabla de conversi√≥n de Celsius a Fahrenheit\n");
    
    celsius = lower;
    while(celsius <= upper) {
        fahr = (9.0/5.0)*celsius + 32;
        printf("%3.0f\t%6.1f\n", celsius, fahr);
        celsius = celsius + step;
    }
    
    return 0;
}
